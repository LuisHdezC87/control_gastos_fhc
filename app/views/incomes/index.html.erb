<p id="notice"><%= notice %></p>

<div class='view-content'>

  <div class="title">
    <h1>Incomes</h1>
  </div>

  <div class="table-incomes-index">
    <table>
      <thead>
        <tr>
          <th>Project</th>
          <th>Amount</th>
          <th>Contact</th>
          <th>Notes</th>
          <th>Date</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @incomes.each do |income| %>
          <tr>
            <td><%= income.project_id %></td>
            <td><%= income.amount %></td>
            <td><%= income.contact_id %></td>
            <td><%= income.notes %></td>
            <td><%= income.date %></td>
            <td><%= link_to 'Show', income %></td>
            <td><%= link_to 'Edit', edit_income_path(income) %></td>
            <td><%= link_to 'Destroy', income, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
<br>

<%= link_to 'New Income', new_income_path %>
