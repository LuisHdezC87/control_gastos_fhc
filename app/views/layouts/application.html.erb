<!DOCTYPE html>
<html>
  <head>
    <title>ControlGastosFhc</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if show_menu?(@project) %>
      <%= render 'layouts/main_menu', project: @project %>
    <% end %>
    <%= yield %>
  </body>
</html>
